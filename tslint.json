{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": [
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "object-literal-sort-keys": false,
    "max-classes-per-file": [true, 99],
    "ban-types": [],
    "ordered-imports": false
  },
  "rulesDirectory": []
}
